   "source": [
    "# GlowScript 3.1 VPython\n",
    "from vpython import *\n",
    "\n",
    "x = -100\n",
    "y = 0\n",
    "t = 0.1\n",
    "cat = sphere(pos=vector(x,y,0), radius=10)\n",
    "for i in range(500):\n",
    "  rate(100)\n",
    "  x = x + (t * (-1) * y)\n",
    "  y = y + (t * x)\n",
    "  cat.pos = vector(x,y,0)"
   ]
